demarrer cqlsh -> cqlsh
create keyspace : cqlsh> CREATE KEYSPACE demo WITH REPLICATION = { 'class' : 'SimpleStrategy', 'replication_factor' : 1 };

create table : cqlsh>  use demo;
CREATE TABLE users (
  firstname text,
  lastname text,
  age int,
  email text,
  city text,
PRIMARY KEY (lastname));

ALTER TABLE users ADD notes list<int>;
